!! Colorscheme

!! Import Path

!! URxvt*perl-lib: /home/datnn/.config/urxvt/ext

! special
URxvt.foreground: #f2e900
URxvt.background: #000000
URxvt.cursorColor: #fdf500
URxvt.cursorUnderline: 1
URxvt.cursorBlink: 1
URxvt.transparent: true
URxvt.shading: 30
URxvt.depth: 32
!! URxvt.cursor

URxvt.iso14755: false
! black
*.color0: #272932
*.color8: #272932

! red
*.color1: #710000
*.color9: #ff1111

! green
*.color2: #1ac5b0
*.color10: #a4bd00

! yellow
*.color3: #b08500
*.color11: #d9a400

! blue
*.color4: #007aff
*.color12: #37ebf3

! magenta
*.color5: #cb1dcd
*.color13: #e455ae

! cyan
*.color6: #37ebf3
*.color14: #33c5ba

! white
*.color7: #d1c5c0
*.color15: #fdf6e3

! Use a truetype font and size.
!xterm*faceName: Rajdhani
!xterm*faceSize: 10

! xFT

!! URxvt Appearance
URxvt.geometry: 80x24
URxvt.letterSpace: 0
URxvt.lineSpace: 0
URxvt.font: \
            xft:Inconsolata Nerd Font:style=Bold:size=12:hinting=true:antialias=true,
!            xft:JetBrains Mono:style=Bold:antialias=true:size=10,
            xft:PowerlineSymbols:style=normal:antialias=true:size=10
!!URxvt.boldFont: xft:SourceCodePro:style=Bold:antialias=true:size=10


!! Common Keybinds for Navigations
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.keysym.Control-Up: \033[1;5A
URxvt.keysym.Control-Down: \033[1;5B
URxvt.keysym.Control-Right: \033[1;5C
URxvt.keysym.Control-Left: \033[1;5D

!! Copy Paste & Other Extensions


URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,resize-font
URxvt.copyCommand: xclip -i -selection clipboard
URxvt.pasteCommand: xclip -o -selection clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.urlLauncher: microsoft-edge-stable
URxvt.underlineURLs: true
URxvt.urlButton: 1

URxvt.scrollBar: 0



URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:      resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show

URxvt.resize-font.step: 2
